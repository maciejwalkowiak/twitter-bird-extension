{
  "name": "Twitter Bird Extension",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "Brings back the bird to Twitter website",
  "homepage_url": "https://github.com/maciejwalkowiak/twitter-bird-extension",
  "icons": {
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/**",
        "https://x.com/**"
      ],
      "js": [
        "content-script.js"
      ]
    }
  ]
}
